#include<iostream>
#include<string.h>
using namespace std;
int main(){
//Addition subtraction multiplication Division program------------------------
	cout<<"Enter 1st number ";
	cin>>var1;
	cout<<"Enter 2nd number ";
	cin>>var2;
	sum= var1+var2;
	cout<<"Sum of Two Number is "<<sum<<endl;

	sub=var1-var2;
	cout<<"Subtract of two numbar is "<<sub<<endl;
	

	mult=var1*var2;
	cout<<"Multiplication of two numbar  is "<<mult<<endl;
	
	div=var1/var2;
	cout<<"Divison of two numbar  is "<<div<<endl;
return 0;
}
